<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quickshop">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuickShop">
        <activity android:name=".PaymentPanel"/>
        <activity android:name=".ProductView" />
        <activity android:name=".addToCart" />
        <activity android:name=".HomePage" />
        <activity android:name=".UserProfile" />
        <activity
            android:name=".LoginSignup"
            android:label="Login And SignUp Page" />
        <activity
            android:name=".MainActivity"
            android:label="Quick Shop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>